<?xml version="1.0"?>
<material>
	<shader assetId="PBR_Standard" vertex="shaders/standard.vert" fragment="shaders/standard.frag"/>

	<properties>
		<property name="albedo" type="vec3" r="0.8" g="0.8" b="0.8" />
		<property name="metallic" type="float" value="0.0" />
		<property name="roughness" type="float" value="0.5" />
		<property name="ao" type="float" value="1.0" />
		<property name="emission" type="vec3" r="0.0" g="0.0" b="0.0" />

		<property name="specular" type="vec3" r="1.0" g="1.0" b="1.0" />
		<property name="shininess" type="float" value="32.0" />
		<property name="ambient" type="vec3" r="0.1" g="0.1" b="0.1" />

		<property name="opacity" type="float" value="1.0" />
		<property name="normalStrength" type="float" value="1.0" />
		<property name="tiling" type="vec2" x="1.0" y="1.0" />
		<property name="offset" type="vec2" x="0.0" y="0.0" />
	</properties>

	<textures>
		<texture2D assetId ="test123"  src="textures/wood_albedo.jpg" />
		<texture2D assetId ="" type="normal" src="textures/wood_normal.jpg"/>
		<texture2D assetId ="" type="roughness" src="textures/wood_roughness.jpg" />
		<texture2D assetId ="" type="metallic" src="textures/wood_metallic.jpg"/>
		<texture2D assetId ="" type="ao" src="textures/wood_ao.jpg"/>
		
		<texture2D assetId ="" type="tank" src="textures/tank.jpg"/>
		<texture2D assetId ="" type="car" src="textures/car.jpg"/>
		<texture2D assetId ="" type="background" src="textures/car.jpg"/>
	</textures>


	<renderState>
		<blend enabled="false" src="src_alpha" dest="one_minus_src_alpha" />
		<depthTest enabled="true" func="less" />
		<depthWrite enabled="true" />
		<cullFace enabled="true" mode="back" />
		<alphaTest enabled="false" ref="0.5" />
	</renderState>

</material>
